        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryVariant">@color/primaryDark</item>
        <item name="colorOnPrimary">@color/primary</item>
       <!-- &lt;!&ndash; Secondary brand color. &ndash;&gt;
